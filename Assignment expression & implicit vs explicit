package main

import "fmt"

func main()  {
	var number = 20
	fmt.Printf("%T", number)
}

package main

import "fmt"

func main()  {
	var number = 20000.98
	fmt.Printf("%T", number)
}

package main

import "fmt"

func main()  {
	number := "Hello"
	fmt.Printf("%T", number)
}

package main

import "fmt"

func main()  {
	var number uint64
	var bl bool

	fmt.Println(number, bl)
}
